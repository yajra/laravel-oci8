{
    "name": "yajra/laravel-oci8",
    "description": "Oracle DB driver for Laravel via OCI8",
    "keywords": [
        "yajra",
        "laravel",
        "oracle",
        "oci8",
        "pdo_oci"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-oci8": ">=3.0.1",
        "ext-pdo": "*",
        "illuminate/database": "^11.0",
        "illuminate/pagination": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "yajra/laravel-pdo-via-oci8": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Yajra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\Oci8\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\Oci8\\Oci8ServiceProvider",
                "Yajra\\Oci8\\Oci8ValidationServiceProvider"
            ]
        }
    },
    "scripts": {
        "docker": "docker run -d -p 49160:22 -p 49161:1521 deepdiver/docker-oracle-xe-11g"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
